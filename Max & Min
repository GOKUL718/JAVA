public class ClassNameHere {
   public static void main(String[] args) {
      String a="GOKUL";
      char[] b=a.toChararray();
      int c;max=0;min=122;
      for(int i=0;i<a.length();i++)
      {
      c=b[i];
         if(c>max)
         {
         max=c;
         }
         if(c<min)
         {
         min=c;
         }
      }
} 
