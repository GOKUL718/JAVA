String a="GOKUL";
      char[] b=a.toCharArray();
      int c,max=0,min=122;
      for(int i=0;i<a.length();i++)
      {
      c=b[i];
         if(c>max)
         {
         max=c;
         }
         if(c<min)
         {
         min=c;
         }
      }
      char MAX=(char)max;
      char MIN=(char)min;
      System.out.println(MAX);
      System.out.println(MIN);
