
class HelloWorld {
    public static void main(String[] args) {
     StringBuffer sb= new StringBuffer("GOKUL");
     sb.append("KASE");
     System.out.println(sb);
     sb.deleteCharAt(5);
     System.out.println(sb);
     sb.reverse();
     System.out.println(sb);
     sb.replace(0,4,"go");
     System.out.println(sb);
    }
}
