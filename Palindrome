import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner scr= new Scanner(System.in);
        String a=scr.nextLine();
        String b="";
       for(int i=a.length()-1;i>=0;i--)
       {
          b=b+a.charAt(i);
       }
       if(a.equals(b))
          {
              System.out.print("PALINDROME");
          }else
          {
             System.out.print("NOT PALINDROME");
          }
          
    }
}
